@startuml Life Span Pregnosis Class Diagrams
enum UserRole {
  ADMIN
  PATIENT
}

class User {
  - String firstName
  - String lastName
  - String email
  - String password
  - UserRole role
  + logIn(): void
  + viewProfile(): void
  + updateProfile(): void
}

class Admin {
  + initiateRegistration(): void
  + exportUserData(): void
  + exportStatistics(): void
}

class Patient {
  - Date dateOfBirth
  - String registrationCode
  - boolean isHIVPositive
  - Date diagnosisDate
  - boolean isOnART
  - Date ARTStartDate
  - String countryISOCode
  - Double computedLifespan
  + computeLifespan(): void
}

User <|-- Admin
User <|-- Patient

@enduml
